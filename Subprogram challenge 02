def menu():
    print("Domination")
    print("Team Deathmatch")
    print("Headquarters")
    option = int(input("Enter your selection, 1,2,3"))
    if option == 1:
        print("Selecting Domination")
    elif option == 2:
        print("Selecting Team Deathmatch")
    elif option == 3:
        print("Selecting Headquarters")
    else:
        print("Error, please try again")
menu()
